{
  "name": "bili-live-listener",
  "type": "module",
  "version": "1.0.5",
  "packageManager": "pnpm@9.0.6",
  "description": "Bilibili直播间消息监听库。",
  "author": "Lu-Jiejie <lu-jiejie@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/Lu-Jiejie/bili-live-listener#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lu-Jiejie/bili-live-listener.git"
  },
  "bugs": "https://github.com/Lu-Jiejie/bili-live-listener/issues",
  "keywords": [
    "bilibili",
    "live",
    "danmu"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "start": "esno src/start.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "nr build",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "bilibili-live-ws": "^6.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.2",
    "@types/node": "^20.11.27",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "simple-git-hooks": "^2.10.0",
    "typescript": "^5.4.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
